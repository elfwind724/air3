{
  "name": "gaia-conspiracy",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { 
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)\\.js$",
      "headers": { 
        "content-type": "application/javascript; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable" 
      }
    },
    {
      "src": "/assets/(.*)\\.css$",
      "headers": { 
        "content-type": "text/css; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable" 
      }
    },
    {
      "src": "/(images|audio|fonts)/.*",
      "headers": { 
        "cache-control": "public, max-age=31536000, immutable" 
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/assets/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "github": {
    "silent": true
  }
} 